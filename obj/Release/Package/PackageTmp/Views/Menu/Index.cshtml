            $("#breakfast_page").addClass("activepagination");
            $("#breakfast_tab").addClass("active");
            $(".tab-pane").removeClass("active");
            $("#breakfast").addClass("in active");

            jQuery('html, body').animate({scrollTop : 0},800);
